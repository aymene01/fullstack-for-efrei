{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"pipeline": {
		"build": {
			"outputs": [".next/**", "!.next/cache/**"],
			"dependsOn": ["lint", "test"]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"cache": true,
			"persistent": true
		},
		"typecheck": {
			"cache": true,
			"persistent": true
		}
	}
}
