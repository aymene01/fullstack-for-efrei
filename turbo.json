{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "cache": true
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": true
    },
    "typecheck": {
      "cache": true
    }
  }
}
